{
  "compilerOptions": {
    "module": "esnext",
    "strict": true,
    "declaration": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "types": ["./typings.d.ts", "jest"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "svl-design": ["src"],
      "svl-design/*": ["src/*", "*"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*"]
}
